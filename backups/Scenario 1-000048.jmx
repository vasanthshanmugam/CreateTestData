          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="PreProcessor-LittlesLaw" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">log.info(&quot;------------------1st example for JSR223 Preprocessor---------------------&quot;);

log.info(&quot;Littles law info&quot;);
log.error(&quot;Littles law error&quot;);
log.warn(&quot;Littles law warn&quot;);

log.info(&quot;------------------2nd example for JSR223 Preprocessor---------------------&quot;);

def text = &quot;Littles law Groovy&quot;;
log.info(text);
log.info(&quot;The example two is combining with text &quot; + text);


log.info(&quot;------------------3rd example for JSR223 Preprocessor---------------------&quot;);

def my_var=vars.get(&quot;Variable_example&quot;);
log.info(&quot;The value of my_var is &quot; + my_var);

log.info(&quot;------------------4th example for JSR223 Preprocessor---------------------&quot;);
vars.put(&quot;MY_VAR&quot;,&quot;LITTLESLAW&quot;);


log.info(&quot;-------------------5th example-----------------------------------------&quot;);
def text06 =&quot;example6&quot;;
vars.put(&quot;My_text&quot;,text06);


log.info(&quot;-----------------------6th example for JSR22 Preprocessor--------------------&quot;);

def my_number_06=vars.get(&quot;Variable_example&quot;).toInteger();

def new_number=3;

def add = my_number_06+new_number;
def sub = my_number_06-new_number;
def mul = my_number_06 * new_number;
def div = my_number_06 / new_number;

vars.put(&quot;My_number_06&quot;,add.toString());
vars.put(&quot;Subtraction&quot;,sub.toString());
vars.put(&quot;Multiplication&quot;,mul.toString());
vars.put(&quot;Multiplication&quot;,div.toString());


</stringProp>
